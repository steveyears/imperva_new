        {% endblock %}
    </head>
    {{include('page_elements/top_nav.twig') }}
    <body>
        <mnain>
            {% block body %}{% endblock %}
        </mnain>
        <!-- Footer -->
            <!-- Bottom Nav -->
            {{include('page_elements/bottom_nav.twig') }}
            <!-- Bottom Contact Us -->
            {{include('page_elements/bottom_contact.twig') }}
            <!-- Bottom Legal -->
            {{include('page_elements/legal.twig') }}
    </body>
</html>
